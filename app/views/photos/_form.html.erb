<%= form_with(model: photo, local: true) do |form| %>

  <div class="field">
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.file_field :photos, multiple: true %>
  </div>


  <div class="actions">
    <button type="submit" class="btn btn-secondary">Submit</button>
  </div>
<% end %>
